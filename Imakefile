MAKEFLAGS += -s
CC = g++ -std=c++14 -Wall -pedantic -O2 -fopenmp 
SourceDir=src/

CXXFLAGS=-g -std=c++14 -Wall -pedantic -O2 -fopenmp 
SRCS= = src/main.cpp src/graph.cpp src/GeoRef.cpp src/geometrie.cpp src/stats.cpp src/model.cpp
OBJS = src/main.o src/graph.o src/GeoRef.o src/geometrie.o src/stats.o src/model.o
LIBS = -lgdal -larmadillo -fopenmp

AllTarget(FracG)
NormalProgramTarget(FracG,$(OBJS),NullParameter,$(LIBS),NullParameter)  
